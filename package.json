{
  "name": "adonis-eos-starter",
  "version": "0.1.0",
  "description": "A high-performance, SEO-first CMS starter kit built with AdonisJS 6, Inertia, React, and PostgreSQL. Turnkey solution with modular content system, RBAC, i18n, AI agents, and more.",
  "keywords": [
    "adonisjs",
    "cms",
    "starter-kit",
    "headless-cms",
    "content-management",
    "inertia",
    "react",
    "postgresql",
    "seo",
    "i18n"
  ],
  "type": "module",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SPACED-MAN/adonis-eos-starter.git"
  },
  "author": "Tyler Shingleton",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "mcp": "node ace mcp:serve --transport=sse --host=127.0.0.1 --port=8787",
    "mcp:sse": "node ace mcp:serve --transport=sse --host=127.0.0.1 --port=8787",
    "mcp:stdio": "node ace mcp:serve --transport=stdio",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "sync:starter": "tsx scripts/sync-from-starter.ts",
    "sync:starter:commit": "tsx scripts/sync-from-starter.ts"
  },
  "imports": {
    "#actions/*": "./app/actions/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#helpers/*": "./app/helpers/*.js",
    "#dtos/*": "./app/dtos/*.js",
    "#modules/*": "./app/modules/*.js",
    "#types/*": "./app/types/*.js",
    "#fields/*": "./app/fields/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@faker-js/faker": "^10.1.0",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.24",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.15.18",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^4.1.17",
    "ts-node-maintained": "^10.9.5",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@adocasts.com/actions": "^1.0.5",
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^3.1.1",
    "@adonisjs/lucid": "^21.6.1",
    "@adonisjs/redis": "^9.2.0",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/shield": "^8.2.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.745.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@google/generative-ai": "^0.24.1",
    "@inertiajs/react": "^2.2.15",
    "@lexical/link": "^0.38.2",
    "@lexical/list": "^0.38.2",
    "@lexical/react": "^0.38.2",
    "@lexical/rich-text": "^0.38.2",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vinejs/vine": "^3.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "edge.js": "^6.2.1",
    "lexical": "^0.38.2",
    "lucide-react": "^0.553.0",
    "luxon": "^3.7.2",
    "marked": "^17.0.1",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
